{
  "name": "totem",
  "version": "1.5.0",
  "private": false,
  "scripts": {
    "start:dev": "vue-cli-service serve --port 5000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "npm run build && npm run start",
    "start": "node server.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.6.8",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.0",
    "eslint-plugin-vue": "^5.2.2",
    "vue-template-compiler": "^2.6.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
